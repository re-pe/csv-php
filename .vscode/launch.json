  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Listen for XDebug",
      "type": "php",
      "request": "launch",
      "port": 9000
    },
    {
      "name": "Launch main.php",
      "type": "php",
      "request": "launch",
      "program": "${workspaceRoot}/main.php",
      "ignore": ["**/vendor/**/*.php"],
      "cwd": "${fileDirname}",
      "port": 9000
    }
  ]
}
